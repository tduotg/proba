{% block errorAccess %}
    <div class="modal fade" tabindex="-1" role="dialog" aria-labelledby="gridSystemModalLabel" id="access_dialog" data-backdrop="false">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="gridSystemModalLabel"> {{'Намальований полігон перетинає кордони'|trans}}</h4>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn" data-dismiss="modal" id="errorclose">Закрити</button>
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
{% endblock errorAccess %}